<Window x:Class="GUI_Geruest.programmStarten"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI_Geruest"
        mc:Ignorable="d"
        Title="Ausführen" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Öffnen :" HorizontalAlignment="Left" Margin="22,20,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxProgramm" Height="23" Margin="84,23,17,0" TextWrapping="Wrap" VerticalAlignment="Top" KeyDown="textBoxProgramm_KeyDown" ToolTip="Programmname eingeben"/>
        <Button x:Name="button" Content="Ausführen" HorizontalAlignment="Left" Margin="200,68,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
